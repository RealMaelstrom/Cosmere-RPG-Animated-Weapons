{
    "name": "cosmere-rpg-animated-weapons",
    "version": "1.1.1",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "npx rollup -c",
        "build:watch": "npx rollup -c -w",
        "build:pack": "npm run build && npm run pack",
        "build:release": "npm run build -- --release && npm run pack && npm run zip",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "link": "node ./scripts/link-foundry",
        "pack": "node ./scripts/pack",
        "unpack": "node ./scripts/unpack",
        "import": "node ./scripts/import",
        "postinstall": "npm run build:pack"
    },
    "devDependencies": {
        "@foundryvtt/foundryvtt-cli": "^1.0.3",
        "@rollup/plugin-commonjs": "^26.0.1",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.3.1",
        "@rollup/plugin-replace": "^5.0.7",
        "archiver": "^7.0.1",
        "prompts": "^2.4.2",
        "rollup": "^4.21.0",
        "rollup-plugin-copy": "^3.5.0",
        "rollup-plugin-scss": "^4.0.0"
    },
    "engines": {
        "node": "^20"
    },
    "dependencies": {
        "axios": "^1.7.7",
        "change-case": "^5.4.4",
        "cheerio": "^1.0.0"
    }
}
